{
    "name": "helsingborg-stad/modularity-sections",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Thulin",
            "email": "sebastian.thulin@helsingborg.se"
        },
        {
            "name": "Nikolas Ramstedt",
            "email": "nikolas.ramstedt@helsingborg.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-theme",
                "type:wordpress-plugin",
                "type:acf-plugin"
            ]
        }
    },
    "require": {
        "helsingborg-stad/acf-export-manager": ">=1.0.0",
        "helsingborg-stad/municipio": "^6.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/helsingborg-stad/acf-export-manager.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "ModularitySections\\": "source/php/"
        }
    },
    "version": "4.0.1",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}